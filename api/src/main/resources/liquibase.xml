<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.9"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.9
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.9.xsd">

    <changeSet author="mindflow" id="201301272147-1">
        <createTable tableName="oncologypoc_subencounter">
           
            <column autoIncrement="true" name="sub_encounter_id" type="INT">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            
            <column name="encounter_id" type="INT">
            	<constraints nullable="false"/>
            </column>

        </createTable>
        <rollback>
            <dropTable tableName="oncologypoc_subencounter"/>
        </rollback>
    </changeSet>
    <changeSet author="mindflow" id="201301272147-2">
        <addForeignKeyConstraint baseColumnNames="encounter_id" baseTableName="oncologypoc_subencounter"
                                 constraintName="encounter_for_this_subencounter_intercepted" deferrable="false" initiallyDeferred="false"
                                 referencedColumnNames="encounter_id" referencedTableName="encounter"/>
    </changeSet>
</databaseChangeLog>